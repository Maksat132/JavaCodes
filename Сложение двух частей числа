class Solution {
    public int minimumSum(int num) {
        String str = Integer.toString(num);
        int index = str.length() / 2;
        String strPart1 = str.substring(0, index);
        String strPart2 = str.substring(index);
        int intPart1 = Integer.parseInt (strPart1);
        System.out.println(intPart1);
        int intPart2 = Integer.parseInt(strPart2);
        System.out.println(intPart2);
        
        return intPart1+intPart2;
    }
}
